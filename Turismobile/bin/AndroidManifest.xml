                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.turismobile.cargadatos.Restarantes"
            android:label="@string/title_activity_restarantes" >
